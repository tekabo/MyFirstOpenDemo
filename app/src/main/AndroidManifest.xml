        <activity android:name=".TestXutilsActivity" />
        <activity android:name=".DaggerTestActivity" />
        <activity android:name=".GreenDaoTestActivity" />
        <activity android:name=".LitepalTestActivity" />
        <activity android:name=".RetrofitTestActivity" />
        <activity android:name=".OkhttpActivity" />
        <activity android:name=".LifeCycleTestActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>