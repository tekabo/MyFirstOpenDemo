<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/test_dialog_library"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="test_dialog_library"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <Button
        android:id="@+id/test_merge_library"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="test_merge_library"
        android:layout_marginTop="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/test_dialog_library" />
    <Button
        android:id="@+id/test_lifecycle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="test_life_cycle"
        android:layout_marginTop="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/test_merge_library" />
    <Button
        android:id="@+id/test_call"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="test_call_phone"
        android:layout_marginTop="10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/test_lifecycle" />
</androidx.constraintlayout.widget.ConstraintLayout>